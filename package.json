{
  "name": "RossHathaway-reviews",
  "version": "1.0.0",
  "description": "Amazon product page review component",
  "author": "Ross Hathaway",
  "license": "free",
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "client:dev": "webpack-dev-server",
    "server": "nodemon server/index.js"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js, jsx}",
      "!**/node_modules/**",
      "!./public/**"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 50,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "css-loader": "^2.1.0",
    "faker": "^4.1.0",
    "file-system": "^2.2.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
